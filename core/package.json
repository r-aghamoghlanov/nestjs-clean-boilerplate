{
  "name": "@backend/core",
  "version": "1.0.0",
  "description": "Core layer for the application",
  "exports": {
    "./cache/*": {
      "types": "./src/cache/*.ts",
      "default": "./dist/cache/*.js"
    },
    "./common/*": {
      "types": "./src/common/*.ts",
      "default": "./dist/common/*.js"
    },
    "./user/*": {
      "types": "./src/user/*.ts",
      "default": "./dist/user/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "Rustam Aghamoghlanov",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "typescript": "~5.9.2"
  }
}
